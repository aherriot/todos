{
  "name": "todos",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.2.7",
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@reduxjs/toolkit": "^1.4.0",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.2",
    "@testing-library/user-event": "^12.2.2",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.19.4",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-redux": "^7.1.11",
    "@types/react-router-dom": "^5.1.6",
    "apollo-server-express": "^2.19.0",
    "apollo3-cache-persist": "^0.8.0",
    "express": "^4.17.1",
    "graphql": "^15.4.0",
    "immer": "^8.0.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "postcss-cli": "^8.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "tailwindcss": "^2.0.1-compat",
    "typescript": "^4.0.5",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "build:style:dev": "postcss src/styles/index.css -o src/styles/generated/tailwind.css --env development",
    "build:style:prod": "postcss src/styles/index.css -o src/styles/generated/tailwind.css --env production",
    "prestart": "npm run build:style:dev",
    "start": "react-scripts start",
    "start:server": "NODE_ENV=development nodemon --ignore '*.test.js' --watch server server/index.js",
    "prebuild": "npm run build:style:prod",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "proxy": "http://localhost:8000",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  }
}
